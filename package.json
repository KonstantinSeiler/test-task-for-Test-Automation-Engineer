{
  "name": "test-task-for-test-automation-engineer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "cy": "yarn cy:open",
    "cy:open": "cross-env NODE_ENV=test ELECTRON_EXTRA_LAUNCH_ARGS=--lang=ru cypress open --config-file cypress.config.ts --browser electron --e2e",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:js": "yarn lint:js:src && yarn lint:js:cypress",
    "lint:js:cypress": "eslint cypress --config ./cypress/.eslintrc.json --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0",
    "lint:js:src": "eslint src --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0",
    "lint:ts": "yarn lint:ts:src && yarn lint:ts:cypress",
    "lint:ts:cypress": "tsc --project cypress/tsconfig.json",
    "lint:ts:src": "tsc",
    "prepare": "husky"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "cross-env": "7.0.3",
    "cross-var": "^1.1.0",
    "cypress": "13.14.2",
    "cypress-on-fix": "^1.0.3",
    "cypress-real-events": "^1.13.0",
    "cypress-wait-until": "3.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.6.2"
  }
}
